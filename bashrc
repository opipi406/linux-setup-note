# .bashrc

# Aliases
alias ll='ls -l'
alias la='ls -a'
alias lla='ls -la'
alias cls='clear'
alias duu='du -hd 1 | sort -hr'

alias py='python'

alias d='docker'
alias dc='docker compose'
alias dcu='docker compose up -d'
alias dcd='docker compose down'
alias dcr='docker compose restart'
alias dcx='docker compose exec'
